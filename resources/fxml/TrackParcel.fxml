      <Button fx:id="tbtn" layoutX="332.0" layoutY="264.0" mnemonicParsing="false" onAction="#goToTrackingDetails" prefHeight="34.0" prefWidth="108.0" text="Track">
         <font>
            <Font name="Cambria" size="18.0" />
         </font>
      </Button>
      <TextArea fx:id="detailArea" layoutX="60.0" layoutY="316.0" prefHeight="160.0" prefWidth="380.0" promptText="Tracking Details will appear here" />
